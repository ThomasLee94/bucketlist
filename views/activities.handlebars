{{#each activities}}
    <div class="container activities-box">
        <p><b id="title">Title: </b>{{title}}</p>
        <p><b id="description">Description: </b>{{description}}</p>
        <p><b id="coordinates">Coordinates: </b>({{longitude}},{{latitude}})</p>
        <p><b id="address">Address: </b><address>{{address}}</address></p>
    </div>
{{/each}}